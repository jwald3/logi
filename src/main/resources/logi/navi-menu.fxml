<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>

<MenuBar fx:id="naviMenu" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logi.controllers.NavMenuController">
   <menus>
      <Menu mnemonicParsing="false" text="File">
         <items>
            <Menu mnemonicParsing="false" text="New">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#onAddNewTruckMenuItemClick" text="Truck" />
                  <MenuItem mnemonicParsing="false" onAction="#onAddNewFacilityMenuItemClick" text="Facility" />
                  <MenuItem mnemonicParsing="false" onAction="#onAddNewTripMenuItemClick" text="Trip" />
               </items>
            </Menu>
            <MenuItem mnemonicParsing="false" text="Close" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="View">
         <items>
            <MenuItem mnemonicParsing="false" onAction="#onViewTrucksMenuItemClick" text="Trucks" />
            <MenuItem mnemonicParsing="false" onAction="#onViewFacilitiesMenuItemClick" text="Facilities" />
            <MenuItem mnemonicParsing="false" onAction="#onViewTripsMenuItemClick" text="Trips" />
         </items>
      </Menu>
   </menus>
</MenuBar>
