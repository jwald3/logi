<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="rootID" maxHeight="475.0" maxWidth="600.0" minHeight="475.0" minWidth="600.0" prefHeight="475.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logi.controllers.AddFacilityController">
    <top>
        <fx:include source="navi-menu.fxml" />
    </top>
    <center>
        <VBox alignment="TOP_CENTER" maxWidth="200.0" minWidth="200.0" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <children>
                <Label text="Facility Name">
                    <VBox.margin>
                        <Insets bottom="5.0" left="-100.0" right="5.0" top="100.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="facilityNameTextField" maxHeight="30.0" maxWidth="200.0" minHeight="30.0" minWidth="200.0" prefHeight="30.0" prefWidth="200.0" promptText="Enter Facility Name" />
                <Label text="Facility Address">
                    <VBox.margin>
                        <Insets bottom="5.0" left="-100.0" right="5.0" top="5.0" />
                    </VBox.margin>
                </Label>
                <TextField fx:id="facilityAddressTextField" maxHeight="30.0" maxWidth="200.0" minHeight="30.0" minWidth="200.0" prefHeight="30.0" prefWidth="200.0" promptText="Enter Facility Address" />
                <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="30.0" maxWidth="100.0" minHeight="30.0" minWidth="100.0" mnemonicParsing="false" onAction="#submitFacilityForm" prefHeight="30.0" prefWidth="100.0" text="Add Facility" textAlignment="CENTER">
                    <VBox.margin>
                        <Insets bottom="5.0" top="10.0" />
                    </VBox.margin>
                </Button>
            <Button alignment="CENTER" contentDisplay="CENTER" maxHeight="30.0" maxWidth="100.0" minHeight="30.0" minWidth="100.0" mnemonicParsing="false" onAction="#viewFacilities" prefHeight="30.0" prefWidth="100.0" text="View Facilities" />
            </children>
        </VBox>
    </center>
    <left>
        <Region prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
    </left>
    <right>
        <Region prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
    </right>
</BorderPane>
